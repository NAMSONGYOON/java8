package sec1;
//프로그램의 모든 명령은 Op-code와 Operand로 이루어짐
//a = a + b;
//+ : 연산(명령) -> Op-code
//a, b -> Operand
//제0주소 명령형식~제3주소 명령형식
public class Oper1 {
	public static void main(String[] args) {
	int a=20, b=30;
	System.out.println(a);
	prt();
	
	}
	
	public static void prt() {
		System.out.println("data");
		
	}
}

package sec1;

public class Car {
	String company;
	String model;
	String color;
	int maxSpeed;
	int speed;
	int rpm;
	Body body;
	Engine engine;
	Tire tire;
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public int getMaxSpeed() {
		return maxSpeed;
	}
	public void setMaxSpeed(int maxSpeed) {
		this.maxSpeed = maxSpeed;
	}
	
	
}
